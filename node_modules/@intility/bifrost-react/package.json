{
  "name": "@intility/bifrost-react",
  "version": "2.13.0",
  "description": "",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "watch": "run-s clean:build build:watch",
    "build": "run-s clean:build build:cjs build:es build:types build:css",
    "build:watch": "run-p build:types:watch build:cjs:watch build:es:watch build:css:watch",
    "build:cjs": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --env-name cjs --root-mode upward",
    "build:cjs:watch": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --env-name cjs --watch --root-mode upward",
    "build:es": "babel src --out-dir es --extensions \".ts,.tsx,.js\" --env-name es --root-mode upward",
    "build:es:watch": "babel src --out-dir es --extensions \".ts,.tsx,.js\" --env-name es --watch --root-mode upward",
    "build:types": "tsc",
    "build:types:watch": "tsc --watch",
    "build:css": "parcel build src/bifrost-app.css --public-url . --no-minify --no-cache",
    "build:css:watch": "parcel watch src/bifrost-app.css --public-url . --no-cache",
    "clean:build": "rimraf dist es types",
    "bump:next": "npm version prerelease --preid=next && git push --tags"
  },
  "types": "types/index",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.12.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@tippyjs/react": "^4.2.0",
    "@types/react-datepicker": "^4.1.6",
    "@types/react-modal": "^3.10.5",
    "@types/react-select": "^4.0.13",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "focus-visible": "^5.1.0",
    "prop-types": "^15.7.2",
    "react-animate-height": "^2.0.23",
    "react-datepicker": "^4.3.0",
    "react-modal": "^3.11.2",
    "react-select": "^4.2.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.62",
    "@types/react": "16.9.34",
    "sass": "^1.27.0"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "gitHead": "1a025e6bd7e3607cfa1007fdb0c717d330beda57"
}
